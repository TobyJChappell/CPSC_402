PDir. Dir ::= "#include" "<" Ident ">" ;
comment "//" ;
terminator Stm "" ;
Prog. Program ::= Func ;
Function. Func ::= Type Ident "()" Seq ;
Sequence. Seq ::= "{" [Stm] "}" ;
TInt. Type ::= "int" ;
SReturn. Stm ::= "return" Integer ";" ;
SPrint. Stm ::= NS "<<" String "<<" NS ";" ;
Namespace. NS ::= Ident "::" Ident ;
